	<include refid="com.s1.arch.reminders.dao.AlertReminderMapper.Base_Column_List" />
	FROM (
	SELECT
	<include refid="com.s1.arch.reminders.dao.AlertReminderMapper.Base_Column_List" />
	FROM BNKALRTRMNDR
	WHERE BNKALRTSBSCRPTNKY IN
	(SELECT BNKALRTSBSCRPTNKY
	FROM BNKALRTSBSCRPTN
	WHERE BNKUserky IN
	(SELECT BNKUSERKY AS userKy
	FROM ARCUSERAUTHNTICN a,
	ARCIDENTITY b,
	BNKUSER d,
	BNKCustomer e
	WHERE a.ARCIDENTITYKY = b.ARCIDENTITYKY
	AND b.ARCPRINCIPALKY = d.ARCPRINCIPALKY
	AND d.BNKCUSTOMERKY = e.BNKCUSTOMERKY
	AND e.CORORGKYFI = #{affiliateOid} )
	) ) where NEXTOCCURRENCEDTTM &lt;= #{nextOccurrenceBefore.time:TIMESTAMP}
	fetch first ${chunkSize} rows only
</select>

